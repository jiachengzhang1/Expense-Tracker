{
  "presets": ["@babel/preset-react", "@babel/preset-env"],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@api": "./src/api",
          "@components": "./src/components",
          "@utils": "./src/utils",
          "@pages": "./src/pages",
          "test": "./test"
        }
      }
    ]
  ]
}
